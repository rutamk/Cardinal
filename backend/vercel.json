{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/home",
      "dest": "/"
    },
    {
      "src": "/login",
      "dest": "/"
    },
    {
      "src": "/signup",
      "dest": "/"
    }
  ],
  "rewrites":[
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
